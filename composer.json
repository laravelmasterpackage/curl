{
    "name": "laravelmasterpackage/curl",
    "description": "Custom PHP Curl library for the Laravel framework - developed by LaravelMasterPackage",
    "version": "1.0.0",
    "keywords": ["LaravelMasterPackage", "Laravel", "Curl"],
    "homepage": "http://laravelmasterpackage.be",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@laravelmasterpackage.be"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "illuminate/support": ">=4.0"
    },

    "autoload": {
        "psr-4": {
            "LaravelMasterPackage\\Curl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMasterPackage\\Curl\\CurlServiceProvider"
            ],
            "aliases": {
                "Curl": "LaravelMasterPackage\\Curl\\Facades\\Curl"
            }
        }
    }
}
