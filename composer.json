{
  "name": "laravelmasterpackage/curl",
  "description": "Custom PHP Curl library for the Laravel framework - developed by laravelmasterpackage",
  "type": "library",
  "version": "1.0.0",
  "require": {
    "php": ">=5.4.0"
  },
  "license": "MIT",
  "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "illuminate/support": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "Ixudra\\Curl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ixudra\\Curl\\CurlServiceProvider"
            ],
            "aliases": {
                "Curl": "Ixudra\\Curl\\Facades\\Curl"
            }
        }
    }
}
